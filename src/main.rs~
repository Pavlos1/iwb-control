mod networking;

//use std::vec::Vec;

fn main() {
    let output_ = networking::discover_hosts("118.139.125.255:3629");
    if output_.is_err()
    {
        println!("Broadcast threw an error.");
        return
    }
    let output = output_.unwrap();
    println!("Length is: {}", output.len());
    for i in output
    {
        let (addr, name) = i;
        println!("{}; {}", addr, name);
    }
}
